{
  "entrypoint": "index.html",
  "shell": "src/dict-ui.html",
  "fragments": [
    "src/dict-entry-view.html",
    "src/ui-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "index.html",
    "src/dict-ui.html",
    "src/dict-entry-view.html",
    "src/ui-404.html"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "github",
      "base": "/dict-api/polymer-app/build/github/"
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}